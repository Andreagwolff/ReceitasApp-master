<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.lucas.receitasapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".BoloActivity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARRENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".TortaActivity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARRENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".CookieActivity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARRENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".SorveteActivity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARRENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity android:name=".DocesActivity" />
        <activity android:name=".QuindimActivity" />
        <activity android:name=".SalgadoActivity" />
        <activity android:name=".EsfihaActivity" />
        <activity android:name=".EnroladinhoActivity" />
        <activity android:name=".CoxinhaActivity" />
        <activity android:name=".BifeActivity" />
        <activity android:name=".PernilActivity" />
        <activity android:name=".CaipirinhaActivity" />
        <activity android:name=".MojitoActivity" />
        <activity android:name=".MargaritaActivity" />
        <activity android:name=".TequilaActivity" />
        <activity android:name=".BebidasActivity" />
    </application>

</manifest>